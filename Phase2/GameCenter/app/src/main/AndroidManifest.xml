<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fall18_207project.GameCenter">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="GameCenter"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="fall18_207project.GameCenter.RegisterActivity"></activity>
        <activity android:name="fall18_207project.GameCenter.GameCenterActivity"></activity>
        <activity android:name="fall18_207project.GameCenter.ScoreBoardActivity" ></activity>
        <activity android:name="fall18_207project.GameCenter.StartingActivity"></activity>
        <activity android:name="fall18_207project.GameCenter.GameActivity"></activity>
        <activity android:name="fall18_207project.GameCenter.SlidingTileFinishActivity"></activity>
    </application>

</manifest>